{
    "name": "hadyfayed/siren",
    "description": "Laravel KickStart",
    "keywords": [
        "hadyfayed",
        "laravel",
        "siren",
        "spatie",
        "jetstream",
        "livewire",
        "hotwire",
        "filament",
        "laravel-settings",
        "spatie-laravel-media-library-plugin",
        "spatie-laravel-settings-plugin",
        "spatie-laravel-tags-plugin",
        "spatie-laravel-translatable-plugin",
        "filament-spatie-laravel-backup",
        "filament-spatie-laravel-health",
        "laravel-sluggable",
        "turbo-laravel",
        "filament-shield"
    ],
    "homepage": "https://github.com/hadyfayed/siren",
    "license": "MIT",
    "authors": [
        {
            "name": "Hady Fayed",
            "email": "hadyfayed@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "livewire/livewire": "^v2.10",
        "laravel/jetstream": "^v2.6",
        "tonysm/turbo-laravel": "^1.1",
        "spatie/laravel-package-tools": "^1.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "HadyFayed\\Siren\\": "src",
            "HadyFayed\\Siren\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HadyFayed\\Siren\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HadyFayed\\Siren\\SirenServiceProvider"
            ],
            "aliases": {
                "Siren": "HadyFayed\\Siren\\Facades\\Siren"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
